{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Warning",
      "Function": "Information",
      "Host.Aggregator": "Warning",
      "Host.Results": "Warning",
      "Function.ActivityFunction": "Trace",
      "Function.ActivityFunction.User": "Trace"
    }
  }
}
